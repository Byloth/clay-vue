  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/clay-vue.js"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./dist/clay-vue.umd.cjs"
      }
    }
  },
  "types": "./types/index.d.ts",
  "scripts": {
    "dev": "storybook dev --port 6006 --no-open",
